{
    "configurations": [
        {
            "name": "vscode_stm32_c_cpp_properties",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/src/",
                "~/Projects/NRF52/SDK/components/**",
                "~/Projects/NRF52/SDK/modules/**",
                "~/Projects/NRF52/SDK/integration/**",
                "~/Projects/NRF52/SDK/external/**",
                "${workspaceFolder}/config"
            ],
            "defines": [
                "BOARD_PCA10056",
                "BSP_DEFINES_ONLY",
                "CONFIG_GPIO_AS_PINRESET",
                "ESB_PRESENT",
                "FLOAT_ABI_HARD",
                "NRF52840_XXAA",
                "SOFTDEVICE_PRESENT"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}